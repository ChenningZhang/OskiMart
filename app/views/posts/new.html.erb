	  									<!-- Content -->

	  									<!-- <div class='row'> -->
	  										<div class='col-md-6 col-md-offset-3'>
	  											<%= form_for(@post) do |f| %>
	  											<!-- Title -->

	  											<%= f.label :title %>

	  											<%= f.text_field :title, class: 'form-control' %>

	  											<%= f.label :description %>
	  											<%= f.text_field :description, class: 'form-control' %>


												<div class = "control-label post_buttons">
		  											<%= f.label :category %>
		  											<%= f.select :category, [['Technology', 1], ['Books', 2], ['Furniture', 3], ['Service', 4]], class: 'form-control' %>

		  											<%= f.label :price%>
		  											<%= f.select :price, [['$', 1], ['$$', 2], ['$$$', 3]], class: 'form-control' %>
		                   							
		                   							
		                   							<div class="col-lg-offset-2">

		  												<%= f.submit "Create Post", class: "btn btn-primary" %>
		  											
		  											</div>
		  										</div>


	  											<% end %>
	  										</div>
	  									<!-- </div> -->



	  								</form>
